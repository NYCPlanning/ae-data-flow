{
  "name": "ae-data-flow",
  "version": "0.0.0",
  "description": "ETL for application engineering",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier **/*.ts --write",
    "flow": "npm run drizzle:flow:push && npm run minio:download && npm run shp:convert && npm run pg:flow",
    "drizzle:api:pull": "drizzle-kit introspect --config ./drizzle/api.config.ts",
    "drizzle:flow:push": "drizzle-kit push --config ./drizzle/flow.config.ts",
    "minio:download": "tsx ./minio/download.ts",
    "shp:convert": "tsx ./shp/convert.ts",
    "pg:flow": "npm run pg:source:flow && npm run pg:target:flow",
    "pg:source:flow": "npm run pg:configure && npm run pg:source:create && npm run pg:source:load",
    "pg:configure": "tsx ./pg/query.ts configure",
    "pg:source:create": "tsx ./pg/query.ts source-create",
    "pg:source:load": "tsx ./pg/copy.ts",
    "pg:target:flow": "npm run pg:target:wrap && npm run pg:target:populate",
    "pg:pluto:transform": "tsx ./pg/query.ts pluto-transform",
    "pg:target:wrap": "tsx ./pg/wrap.ts",
    "psql:target:repopulate": "docker compose exec -it db bash repopulate/target-repopulate.sh"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@terraformer/wkt": "^2.2.1",
    "@turf/turf": "^7.0.0",
    "@types/geojson": "^7946.0.14",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.6",
    "@types/pg-copy-streams": "^1.2.5",
    "@types/pg-format": "^1.0.5",
    "@types/shpjs": "^3.4.7",
    "csv-stringify": "^6.5.0",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.22.8",
    "drizzle-orm": "^0.31.2",
    "minio": "^8.0.1",
    "pg": "^8.12.0",
    "pg-copy-streams": "^6.0.6",
    "pg-format": "^1.0.4",
    "prettier": "3.3.2",
    "shpjs": "^5.0.2",
    "tsx": "^4.16.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "zod": "^3.23.8"
  }
}
